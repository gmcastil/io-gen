# This is an XDC comment for LED
set_property PACKAGE_PIN T22 [get_ports {led_pad[0]}]
set_property IOSTANDARD LVCMOS33 [get_ports {led_pad[0]}]
set_property PACKAGE_PIN T23 [get_ports {led_pad[1]}]
set_property IOSTANDARD LVCMOS33 [get_ports {led_pad[1]}]

# BTN0 input
set_property PACKAGE_PIN U19 [get_ports {btn_pad}]
