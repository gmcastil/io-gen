# This is an XDC comment for LED[0]
set_property PACKAGE_PIN T22 [get_ports {led[0]}]
set_property IOSTANDARD LVCMOS33 [get_ports {led[0]}]

# This is an XDC comment for LED[1]
set_property PACKAGE_PIN T23 [get_ports {led[1]}]
set_property IOSTANDARD LVCMOS33 [get_ports {led[1]}]

# BTN0 input
set_property PACKAGE_PIN U19 [get_ports {btn}]
set_property IOSTANDARD LVCMOS33 [get_ports {btn}]

# BTN_BUS_DIFF[0] input  (scalar-as-bus example uses its own text)
set_property PACKAGE_PIN U20 [get_ports {btn_bus[0]}]
set_property IOSTANDARD LVCMOS33 [get_ports {btn_bus[0]}]

# Differential LED[0] outputs
set_property PACKAGE_PIN Y19 [get_ports {led_diff_p[0]}]
set_property IOSTANDARD LVDS_25 [get_ports {led_diff_p[0]}]
set_property PACKAGE_PIN Y20 [get_ports {led_diff_n[0]}]
set_property IOSTANDARD LVDS_25 [get_ports {led_diff_n[0]}]

# Differential LED[1] outputs
set_property PACKAGE_PIN W19 [get_ports {led_diff_p[1]}]
set_property IOSTANDARD LVDS_25 [get_ports {led_diff_p[1]}]
set_property PACKAGE_PIN W20 [get_ports {led_diff_n[1]}]
set_property IOSTANDARD LVDS_25 [get_ports {led_diff_n[1]}]

# BTN_DIFF input pair
set_property PACKAGE_PIN V17 [get_ports {btn_diff_p}]
set_property IOSTANDARD LVDS_25 [get_ports {btn_diff_p}]
set_property PACKAGE_PIN V18 [get_ports {btn_diff_n}]
set_property IOSTANDARD LVDS_25 [get_ports {btn_diff_n}]

# BTN_BUS_DIFF[0] input pair
set_property PACKAGE_PIN U17 [get_ports {btn_bus_diff_p[0]}]
set_property IOSTANDARD LVDS_25 [get_ports {btn_bus_diff_p[0]}]
set_property PACKAGE_PIN U18 [get_ports {btn_bus_diff_n[0]}]
set_property IOSTANDARD LVDS_25 [get_ports {btn_bus_diff_n[0]}]

